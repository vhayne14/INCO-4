
        <div class="container">
            <div class="row">
                <div class="col s12">
                    <div class="card">
                        <div class="card-content">
                            <span class="card-title center">Existing Schedules</span>
                            <div class="row">
                               <table class = "highlight striped responsive-table">
                                    <thead>
                                        <tr>
                                            <th>UserID</th>
                                            <th>Firstname</th>
                                            <th>Lastname</th>
                                            <th>Day</th>
                                            <th>Start</th>
                                            <th>End</th>

                                        </tr>
                                    
                                    </thead>
                                    <tbody>

                                    
                                        <% sched.forEach(sched => { %>
                                            <tr>
                                                <td>
                                                    <a href=" http://localhost:5000/users/<%= sched.id %>"><%= sched.id %></a>
                                                </td>
                                                <td>
                                                    <%= sched.firstname %>
                                                    
                                                </td>
                                                <td>
                                                    <%= sched.lastname %>
                                                    
                                                </td>
                                        
                                                <% let day %>
                                                <% if(sched.day == 1) {%>
                                                    <%  day = 'Monday'; %>
                                                <% } %>

                                                <% if(sched.day == 2) {%>
                                                    <%  day = 'Tuesday'; %>
                                                <% } %>

                                                <% if(sched.day == 3) {%>
                                                    <%  day = 'Wednesday'; %>
                                                <% } %>

                                                <% if(sched.day == 4) {%>
                                                    <%  day = 'Thursday'; %>
                                                <% } %>

                                                <% if(sched.day == 5) {%>
                                                    <%  day = 'Friday'; %>
                                                <% } %>

                                                <% if(sched.day == 6) {%>
                                                    <%  day = 'Saturday'; %>
                                                <% } %>

                                                <% if(sched.day == 7) {%>
                                                    <%  day = 'Sunday'; %>
                                                <% } %>

                                                <td><%= day %></td>
                                                <td><%= sched.start_time %> </td>
                                                <td><%= sched.end_time %> <br></td>
                                        
                                            </tr>
                                        <% }) %>

                                    </tbody>

                                </table> 
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>


   